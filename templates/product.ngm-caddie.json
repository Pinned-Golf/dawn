    "multicolumn_pHTL4n": {
      "type": "multicolumn",
      "blocks": {
        "column_7E8hdq": {
          "type": "column",
          "settings": {
            "title": "Game Changer",
            "text": "<p><em>“The Caddie has completely changed how I play. It’s like having a personal caddie every round. Accurate yardages and an easy interface—love it!”</em></p><p><strong>Eddie, Delray Beach, FL</strong></p>",
            "link_label": "",
            "link": ""
          }
        },
        "column_34PWE7": {
          "type": "column",
          "settings": {
            "title": "Effortless and Accurate",
            "text": "<p><em>“This tablet is so simple to use yet packed with features. Knowing yardages to every spot on the course has been a game-changer for my club selection.”</em></p><p><strong>Chris, Scottsdale, AZ</strong></p>",
            "link_label": "",
            "link": ""
          }
        },
        "column_iRQ3Vx": {
          "type": "column",
          "settings": {
            "title": "Worth Every Penny",
            "text": "<p><em>“The Caddie is now a must-have in my bag. It’s reliable, lightweight, and has saved me strokes with precise distances every round.”</em></p><p><strong>Emily, Palm Beach, FL</strong></p>",
            "link_label": "",
            "link": ""
          }
        },
        "column_LLaAj4": {
          "type": "column",
          "settings": {
            "title": "Confidence at new courses",
            "text": "<p><em>“The Caddie tablet has given me the confidence I need when playing a new course. I know exactly where everything on the course is like i've played it 100 times before.”</em></p><p><strong>Morgan, San Diego, CA</strong></p>",
            "link_label": "",
            "link": ""
          }
        },
        "column_7j9MNE": {
          "type": "column",
          "settings": {
            "title": "Perfect for Golfers of All Levels",
            "text": "<p><em>“Whether you’re just starting out or a seasoned player, the Caddie is a fantastic tool. It’s intuitive, accurate, and perfect for every skill level.”</em></p><p><strong>Bryan, Austin, TX</strong></p>",
            "link_label": "",
            "link": ""
          }
        },
        "column_LxCaqB": {
          "type": "column",
          "settings": {
            "title": "Fly Over",
            "text": "<p>\"<em>The flyover before the hole is the best part. It allows me to map out the hole and figure out what to avoid.\"</em></p><p><strong>Conner, Jupiter, FL</strong></p>",
            "link_label": "",
            "link": ""
          }
        }
      },
