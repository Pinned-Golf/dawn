        "row_W76Ln8": {
          "type": "row",
          "settings": {
            "image": "shopify:\/\/shop_images\/7_8593bb46-0854-4953-b034-d8982498cc9d.jpg",
            "caption": "Connect",
            "heading": "Two Dart Speakers",
            "text": "<p>Getting dialed in is always within reach. Sticking your Prism+ to the cart bar or a club will allow for easy access. <\/p>",
