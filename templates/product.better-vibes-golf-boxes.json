        "row_hqhrD3": {
          "type": "row",
          "settings": {
            "image": "shopify://shop_images/11_03b32129-a4e3-4376-91e4-3bca5384dd6e.jpg",
            "caption": "",
            "heading": "Prism+ Rangefinders",
            "text": "<p>The best rangefinder we have ever created. Word on the course is that it is the most user friendly rangefinder ever created. It's packed with new cutting edge software, a dialed in performance aesthetic and never before seen features.</p>",
            "button_label": "",
            "button_link": ""
